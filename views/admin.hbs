<div class="info">
  <a href="/logout">
    <button type="button" class="btn btn-danger btn-lg logout">Cerrar Sesión</button>
  </a>

  <main class="w-50 m-auto admin-content">
    <ul class="nav nav-tabs" role="tablist">
      <li class="nav-item" role="presentation">
        <a
          class="nav-link"
          data-bs-toggle="tab"
          href="#home"
          aria-selected="false"
          role="tab"
          tabindex="-1"
        >Dar de alta</a>
      </li>
      <li class="nav-item" role="presentation">
        <a
          class="nav-link active"
          data-bs-toggle="tab"
          href="#profile"
          aria-selected="true"
          role="tab"
        >Dar de baja</a>
      </li>
    </ul>
    <div id="myTabContent" class="tab-content p-2">
      <div class="tab-pane fade" id="home" role="tabpanel">
        <form data-bitwarden-watching="1">
          <fieldset>
            <div class="form-group p-2">
              <label for="dealta" class="form-label mt-4">Número de ID
              </label>
              <input
                type="email"
                class="form-control"
                id="dealta"
                aria-describedby="emailHelp"
                placeholder="ID del estudiante"
              />
              <small id="emailHelp" class="form-text text-muted">No compartas el
                Número de ID
              </small>
            </div>
            <button type="submit" class="btn btn-primary">Dar De Alta</button>
          </fieldset>
        </form>
      </div>
      <div class="tab-pane fade active show" id="profile" role="tabpanel">
        <form data-bitwarden-watching="1">
          <fieldset>
            <div class="form-group p-2">
              <label for="debaja" class="form-label mt-4">Número de ID
              </label>
              <input
                type="email"
                class="form-control"
                id="debaja"
                aria-describedby="emailHelp"
                placeholder="ID del estudiante"
              />
              <small id="emailHelp" class="form-text text-muted">No compartas el
                Número de ID
              </small>
            </div>
            <button type="submit" class="btn btn-danger">Dar De Baja</button>
          </fieldset>
        </form>
      </div>
    </div>
  </main>
</div>