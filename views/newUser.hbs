<div class="form"></div>

{{>drawer}}

<h1 class="titulo">Dar de Alta Usuarios</h1>

<form data-bitwarden-watching="1" action="/users/newUser" method="post">
    <fieldset>
        <div class="form-group p-2">
            <label for="dealta" class="form-label mt-4">Número de ID
            </label>
            <input type="email" class="form-control" id="dealta" aria-describedby="emailHelp"
                placeholder="ID del estudiante" />
            <label for="dealta" class="form-label mt-4">Contraseña
            </label>
            <input type="text" class="form-control" id="dealta" aria-describedby="emailHelp"
                placeholder="Contraseña del estudiante" />
            <small id="emailHelp" class="form-text text-muted">No compartas la contraseña
            </small>
        </div>
        <button type="submit" class="btn btn-primary">Dar De Alta</button>
    </fieldset>
</form>